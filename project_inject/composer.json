{
  "name": "example/project-inject",
  "description": "Demo PHP web application for SBOM generation",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0",
    "guzzlehttp/guzzle": "^7.9",
    "ramsey/uuid": "^4.7",
    "monolog/monolog": "^3.7",
    "vlucas/phpdotenv": "^5.6",
    "doctrine/dbal": "^4.0",
    "nesbot/carbon": "^3.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3",
    "fakerphp/faker": "^1.24",
    "phpstan/phpstan": "^1.12",
    "friendsofphp/php-cs-fixer": "^3.64"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "lint": "php-cs-fixer fix --dry-run --diff",
    "analyse": "phpstan analyse src --level=max"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
